{
  "name": "streamify-audio",
  "version": "2.2.4",
  "description": "Dual-mode audio library: HTTP streaming proxy + Discord player (Lavalink alternative). Supports YouTube, Spotify, SoundCloud with audio filters.",
  "main": "index.js",
  "types": "index.d.ts",
  "bin": {
    "streamify": "bin/streamify.js"
  },
  "scripts": {
    "start": "UV_THREADPOOL_SIZE=64 node --noconcurrent-sweeping --max-old-space-size=2048 --expose-gc index.js",
    "dev": "UV_THREADPOOL_SIZE=64 node --noconcurrent-sweeping --max-old-space-size=2048 --expose-gc index.js --dev",
    "test": "node --test tests/*.test.js"
  },
  "keywords": [
    "discord",
    "music",
    "bot",
    "youtube",
    "spotify",
    "soundcloud",
    "audio",
    "streaming",
    "lavalink",
    "lavalink-alternative",
    "discordjs-voice",
    "yt-dlp",
    "ffmpeg"
  ],
  "author": "Lucas",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "opusscript": "^0.0.8"
  },
  "optionalDependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.18.0"
  },
  "peerDependencies": {
    "discord.js": "^14.0.0"
  },
  "peerDependenciesMeta": {
    "discord.js": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LucasCzechia/streamify.git"
  },
  "homepage": "https://github.com/LucasCzechia/streamify#readme",
  "bugs": {
    "url": "https://github.com/LucasCzechia/streamify/issues"
  }
}
